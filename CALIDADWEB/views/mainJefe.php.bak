    // Limpiar las opciones anteriores
    selectElementos.innerHTML = ''; // Limpiar cualquier opción previa

    if (categoriaSeleccionada){
        
        const areaSeleccionada = document.getElementById('area').value;
		const elementos = datosAreas[areaSeleccionada].elementos[categoriaSeleccionada];

        // Agregar las opciones al select de elementos
        elementos.forEach(elemento => {
            const option = document.createElement('option');
            option.value = elemento.id;
            option.textContent = elemento.nombre;
            elementosSelect.appendChild(option);
        });

        
    }  
}

function calcularPorcentaje() {
    const selectElement = document.getElementById('elementos');
    const selectedOptions = selectElement.selectedOptions;

    // Obtener el total de opciones disponibles
    const totalOptions = selectElement.options.length;

    // Calcular el porcentaje de opciones seleccionadas
    const porcentajeDecreciente = ((totalOptions - selectedOptions.length) / totalOptions) * 100;

    // Mostrar el porcentaje en el input con id "porcentaje"
    document.getElementById("porcentaje").value = porcentajeDecreciente.toFixed(2);
}

</script> 
  
  
