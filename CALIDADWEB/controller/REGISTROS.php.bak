if (isset($_POST['formAuditoriaProceso'])) {
	
	

	
    // Recibir los datos del formulario
    $area = $_POST['area'];
    $auditoriaResponsable = $_POST['areaResponsable'];
    $categoria = $_POST['categorias'];
    $auditoriaObservaciones = $_POST['observaciones'];
    $auditoriaFecha = isset($_POST['fecha_creacion']) ? $_POST['fecha_creacion']: date('y-m-d');
    $auditoriaCalidad = $_POST['calidadResponsable'];
	$porcentaje = $_POST['valorPorcentaje'];
	 if (isset($_POST['aspectosCategoria']) && is_array($_POST['aspectosCategoria'])) {
        $elementos_container = implode(", ", $_POST['aspectosCategoria']); // Convertir a cadena separada por comas  
  
    // Conexion a la base de datos (asegúrate de que la conexión esté realizada antes)
    try {
        // Preparar la consulta SQL para insertar datos
        $consultar = "INSERT INTO `auditoria-proceso` (`area`, `categorias`, `observaciones`,  `fecha_creacion`, `areaResponsable`, `calidadResponsable`, `aspectosCategoria`, `valorPorcentaje` )
                      VALUES (:area, :categorias,  :observaciones,  :fecha_creacion, :areaResponsable, :calidadResponsable, :aspectosCategoria, :valorPorcentaje)";
        
        // Preparar la sentencia
        $stmt = $bdc->prepare($consultar);
        
        // Vincular los parámetros
        $stmt->bindParam(':area', $area);
        $stmt->bindParam(':categorias', $categoria);
        $stmt->bindParam(':observaciones', $auditoriaObservaciones);
        $stmt->bindParam(':aspectosCategoria', $elementos_container);
        $stmt->bindParam(':fecha_creacion', $auditoriaFecha);
        $stmt->bindParam(':areaResponsable', $auditoriaResponsable);
        $stmt->bindParam(':calidadResponsable', $auditoriaCalidad);
		$stmt->bindParam(':valorPorcentaje', $porcentaje);
		
        // Ejecutar la consulta
        $stmt->execute();

        // Verificar si se insertó un registro correctamente
        if ($stmt->rowCount()) {
            // Si la inserción fue exitosa, guardar el mensaje de éxito
            $_SESSION['mensaje'] = 'Registro Realizado de Manera Exitosa!!!';
            $_SESSION['tipo'] = 'success';
            $_SESSION['icon'] = 'check-circle-fill';
        } else {
            // Si no se insertó ningún registro, mostrar error
            $_SESSION['mensaje'] = 'Hubo un error al insertar el registro.';
            $_SESSION['tipo'] = 'danger';
            $_SESSION['icon'] = 'exclamation-triangle-fill';
        }
    } catch (PDOException $e) {
        // Si hay un error de conexión o ejecución, mostrar el error
        $_SESSION['mensaje'] = "Error: " . $e->getMessage();
        $_SESSION['tipo'] = 'danger';
        $_SESSION['icon'] = 'exclamation-triangle-fill';
    }
}
